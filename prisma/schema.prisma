  failed_requests     Int?                    @default(0)
  batch_size          Int?                    @default(75)
  include_inactive    Boolean?                @default(false)
  started_at          DateTime?               @db.Timestamp(0)
  completed_at        DateTime?               @db.Timestamp(0)
  created_at          DateTime?               @default(now()) @db.Timestamp(0)
  updated_at          DateTime?               @default(now()) @db.Timestamp(0)
  error_message       String?                 @db.Text
  server_stats        Json?

  @@index([created_at], map: "idx_created_at")
  @@index([job_id], map: "idx_job_id")
  @@index([status], map: "idx_status")
}

model reporting_periods {
  id                 BigInt               @id @default(autoincrement())
  name               String               @db.VarChar(50)
  description        String?              @db.LongText
  days               Int
  campaign_analytics campaign_analytics[]
}

model scheduler_state {
  id               Int       @id @default(1)
  enabled          Boolean?  @default(false)
  interval_hours   Int?      @default(4)
  batch_size       Int?      @default(75)
  include_inactive Boolean?  @default(false)
  created_at       DateTime? @default(now()) @db.Timestamp(0)
  updated_at       DateTime? @default(now()) @db.Timestamp(0)
}

model scheduler_status {
  id             Int       @id @default(autoincrement())
  running        Boolean
  last_run       DateTime? @db.DateTime(6)
  next_run       DateTime? @db.DateTime(6)
  interval_hours Int
  batch_size     Int
  max_workers    Int
  last_updated   DateTime  @db.DateTime(6)
}

model transactions {
  id               Int      @id @default(autoincrement())
  transaction_id   String   @unique(map: "transaction_id") @db.VarChar(50)
  transaction_type String   @db.VarChar(20)
  msisdn           String?  @db.VarChar(20)
  offer_code       String?  @db.VarChar(50)
  timestamp        DateTime @db.DateTime(6)
  raw_data         Json?

  @@index([msisdn], map: "transaction_msisdn_b8d763_idx")
  @@index([timestamp], map: "transaction_timesta_07633a_idx")
  @@index([transaction_type], map: "transaction_transac_ddda52_idx")
  @@index([msisdn], map: "transactions_msisdn_870711b4")
}

model core_client {
  id                  Int       @id @default(autoincrement())
  msisdn              String    @unique(map: "msisdn") @db.VarChar(15)
  offer_code          String?   @default("001045836303") @db.VarChar(50)
  subscription_status String    @default("A") @db.Char(1)
  last_payment_date   DateTime? @db.DateTime(0)
  created_at          DateTime  @db.DateTime(0)
  updated_at          DateTime  @db.DateTime(0)

  @@index([last_payment_date], map: "core_client_last_payment_date")
  @@index([offer_code], map: "core_client_offer_code")
  @@index([subscription_status], map: "core_client_subscription_status")
  @@index([subscription_status, last_payment_date], map: "core_client_subscription_status_last_payment_date")
  @@index([subscription_status, offer_code], map: "core_client_subscription_status_offer_code")
}

model payment_logs {
  id                 Int                        @id @default(autoincrement())
  request_id         String                     @unique(map: "request_id") @db.VarChar(100)
  msisdn             String                     @db.VarChar(15)
  offer_code         String                     @db.VarChar(50)
  endpoint_used      payment_logs_endpoint_used
  response_received  Boolean                    @default(false)
  status_code        String?                    @db.VarChar(10)
  description        String?                    @db.Text
  response_data      Json?
  error_message      String?                    @db.Text
  processing_time_ms Int?
  created_at         DateTime                   @db.DateTime(0)

  @@index([created_at], map: "payment_logs_created_at")
  @@index([endpoint_used], map: "payment_logs_endpoint_used")
  @@index([msisdn], map: "payment_logs_msisdn")
  @@index([response_received], map: "payment_logs_response_received")
  @@index([status_code], map: "payment_logs_status_code")
}

enum processing_jobs_status {
  pending
  running
  completed
  failed
}

enum payment_logs_endpoint_used {
  primary
  fallback
}
